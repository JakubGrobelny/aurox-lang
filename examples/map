define f x :=
    f x
end

define id a :=
    a
end

define map f xs :=
    match xs with
        case [] => []
        case [x | xs] => [f x | map f xs]
    end
end

define filter p xs :=
    match xs with
        case []       => []
        case [x | xs] =>
            if p x
                then [x | filter p xs]
                else filter p xs
            end
    end 
end

define mapped := map {|x| x} [1,2,3,4] end
