import {
    IO
    "file.ax"
}

defop { $ 0 right }

defun ($) f x : (a -> b) -> a -> b = f x.

define π : Float = 3.14.

type Tree : a {
    Leaf
    Node { Tree a, a, Tree a }
}

defun tree_mult_by_π t : Tree Int -> Tree Int =
    match t {
        case Leaf => Leaf
        case Node (l, v, r) => Node (tree_mult_by_π l, π * v, tree_mult_by_π r)
    }








